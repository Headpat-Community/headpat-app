{
  "cli": {
    "version": ">= 9.0.6",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "autoIncrement": true,
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": "versionCode"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6502715063"
      },
      "android": {
        "track": "internal"
      }
    }
  }
}
