{
  "expo": {
    "name": "Headpat",
    "slug": "headpat-app",
    "version": "0.5.0",
    "orientation": "portrait",
    "icon": "./assets/images/headpat_logo.png",
    "scheme": "headpatapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/headpat_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.headpat.app",
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyBlqdKEHTabryf5Et0bZQAtxO10VJMf1go"
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location when open.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app requires access to your location even when closed.",
        "NSLocationAlwaysUsageDescription": "This app requires access to your location when open."
      },
      "associatedDomains": [
        "applinks:headpat.app",
        "applinks:headpat.de",
        "applinks:api.headpat.de"
      ],
      "buildNumber": "27"
    },
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/headpat_logo.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD4v3MV8UByXinnFD_9Ck_QBI2tS9VFR3Q"
        }
      },
      "package": "com.headpat.app",
      "versionCode": 28
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "904378a3-321c-4abe-9c08-48274d5f6267"
      }
    },
    "owner": "expo-headpat",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/904378a3-321c-4abe-9c08-48274d5f6267",
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "plugins": [
      "expo-localization",
      "expo-apple-authentication",
      "expo-background-fetch",
      [
        "expo-task-manager",
        {
          "ios": {
            "minimumOSVersion": "14"
          }
        }
      ],
      [
        "expo-background-fetch",
        {
          "ios": {
            "minimumOSVersion": "14"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Headpat to use your location.",
          "locationAlwaysPermission": "Allow Headpat to use your location.",
          "locationWhenInUsePermission": "Allow Headpat to use your location.",
          "photosPermission": "Allow Headpat to access your photos.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your fellow headpatters."
        }
      ],
      "expo-video"
    ]
  }
}
